  "verses": [
    {
      "text": "\fLovpris JHVH, alle folk!\fPris ham, alle folkeslag!",
      "title": "Lovpris JHVH",
      "number": 1,
      "version": 1499983200000,
      "footnotes": [
        {
          "text": "Kan også oversættes: »hedninger«, men det er næppe meningen her.",
          "type": "T",
          "position": 25
        }
      ]
    },
    {
      "text": "\fFor hans nåde mod os har været mægtig,\fog JHVHs troskab varer til evig tid.\fHalleluja!",
      "number": 2,
      "version": 1499983200000,
      "footnotes": [
        {
          "text": "nåde",
          "type": "N",
          "position": 14
        },
        {
